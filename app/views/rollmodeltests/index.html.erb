<p id="notice"><%= notice %></p>

<h1>Rollmodeltests</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @rollmodeltests.each do |rollmodeltest| %>
      <tr>
        <td><%= rollmodeltest.name %></td>
        <td><%= rollmodeltest.email %></td>
        <td><%= link_to 'Show', rollmodeltest %></td>
        <td><%= link_to 'Edit', edit_rollmodeltest_path(rollmodeltest) %></td>
        <td><%= link_to 'Destroy', rollmodeltest, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Rollmodeltest', new_rollmodeltest_path %>
